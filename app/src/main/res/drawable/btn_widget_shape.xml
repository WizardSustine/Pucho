<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:width="50dp" android:height="50dp">
        <shape android:shape="oval" >
            <solid android:color="#507904" />
            <corners android:radius="16dp" /> <!-- Example: Rounded corners with 16dp radius -->
            <stroke android:color="#000000" android:width="2dp" />
        </shape>
    </item>
    <item android:width="52dp" android:height="40dp" android:left="0dp" android:top="0dp" >
        <rotate
            android:fromDegrees="15"
            android:toDegrees="0"
            android:pivotX="0%"
            android:pivotY="0%" >

            <bitmap android:src="@mipmap/ic_launcher_foreground" />
        </rotate>
    </item>
</layer-list>

